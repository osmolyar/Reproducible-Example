{
  "name": "selenium-skeleton",
  "version": "1.0.0",
  "description": "Selenium Skeleton Project",
  "main": "",
  "scripts": {
    "setup": "npm install; node ./selenium/download.js;",
    "start": "cd selenium; java -jar SeleniumGridExtras-1.12.10-SNAPSHOT-jar-with-dependencies.jar;",
    "desktop": "wdio desktop.config.js",
    "android": "wdio android.config.js",
    "ios": "wdio ios.config.js"
  },
  "keywords": [
    "Selenium",
    "InterSystems",
    "QD",
    "HSPD15",
    "Testing",
    "End-To-End"
  ],
  "author": "Olga Smolyar, Henry Zou",
  "license": "ISC",
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true
    },
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules": {
      "semi": 2
    }
  },
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/polyfill": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@babel/register": "^7.8.3",
    "@types/chai": "^4.2.7",
    "@types/cucumber": "^6.0.0",
    "@types/lodash": "^4.14.149",
    "@types/yargs": "^15.0.3",
    "@wdio/allure-reporter": "^5.16.11",
    "@wdio/cli": "^5.16.15",
    "@wdio/cucumber-framework": "^5.16.15",
    "@wdio/junit-reporter": "^5.16.11",
    "@wdio/local-runner": "^5.16.15",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.16.11",
    "@wdio/sync": "^5.16.15",
    "chai": "^4.2.0",
    "colors": "^1.4.0",
    "deepmerge": "",
    "lodash": "^4.17.15",
    "moment": "^2.24.0",
    "typescript": "^3.7.3",
    "wdio-cucumberjs-json-reporter": "^1.1.2",
    "wdio-edgedriver-service": "^1.0.0",
    "yargs": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^13.13.4"
  }
}
